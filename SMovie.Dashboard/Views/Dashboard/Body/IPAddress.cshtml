@using SMovie.Dashboard.Constants
@using SMovie.Domain.Models
@model IEnumerable<BlackIP>

<div class="content-inner container-fluid pb-0" id="page_layout">
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-header border-bottom d-flex justify-content-between align-items-center pb-3">
                    <div class="d-flex align-items-center pt-3">
                        <div class="form-group">
                            aa
                        </div>
                    </div>
                </div>

                <div class="custom-table-effect table-responsive  border rounded py-4">
                    <table class="table mb-0" id="datatable" data-toggle="data-table">
                        <thead>
                            <tr class="bg-white">
                                <th scope="col">IP</th>
                                <th scope="col">Note</th>
                                <th scope="col">Status</th>
                                <th scope="col">DateCreated</th>
                                <th scope="col">Action</th>
                            </tr>
                        </thead>
                        <tbody id="movie-list-table">
                            @{
                                await Html.RenderPartialAsync(ConstantComponent.DisplayIP, (IEnumerable<BlackIP>)Model);
                            }

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>